package org.class2;

import java.sql.PseudoColumnUsage;

public class E19Vars {

    public static void main(String[] args) {
        String firstName="Asghar";
        String lastName="Nazir";
        String mobileEmail="abcd@gmail.com";
        String password="123abcdr";
        int day=1;
        String month="Jan";
        int year=2000;
        char gender='M';
        System.out.println("My name is "+firstName+" "+lastName);
        System.out.println("My email is "+mobileEmail);
        System.out.println("My Password is "+ password);
        System.out.println("I was born on "+day+"-"+month+"-"+year);
        System.out.println("My Gender is "+gender);
    }

}
